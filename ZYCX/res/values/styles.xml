<resources xmlns:android="http://schemas.android.com/apk/res/android">

    <!--
        Base application theme, dependent on API level. This theme is replaced
        by AppBaseTheme from res/values-vXX/styles.xml on newer devices.
    -->
    <style name="AppBaseTheme" parent="android:Theme.Light">
        <!--
            Theme customizations available in newer API levels can go in
            res/values-vXX/styles.xml, while customizations related to
            backward-compatibility can go here.
        -->
    </style>

    <!-- Application theme. -->
    <style name="AppTheme" parent="AppBaseTheme">
        <!-- All customizations that are NOT specific to a particular API-level can go here. -->
    </style>

    <style name="top_group">
        <item name="android:layout_height">30dp</item>
        <item name="android:layout_width">0dp</item>
        <item name="android:layout_weight">1</item>
        <item name="android:textSize">16sp</item>
        <item name="android:textColor">@drawable/qq_constact_font</item>
        <item name="android:background">@drawable/qq_constact_group</item>
    </style>

    <style name="top_all">
        <item name="android:layout_height">30dp</item>
        <item name="android:layout_width">0dp</item>
        <item name="android:layout_weight">1</item>
        <item name="android:textSize">16sp</item>
        <item name="android:textColor">@drawable/qq_constact_font</item>
        <item name="android:background">@drawable/qq_constact_all</item>
    </style>

    <style name="customProgressBar" parent="@android:style/Widget.ProgressBar.Small">
        <item name="android:indeterminateDrawable">@drawable/custom_progress_bar</item>
        <item name="android:layout_width">21dip</item>
        <item name="android:layout_height">21dip</item>
        <item name="android:layout_gravity">center</item>
    </style>

    <style name="popwin_anim_style">
        <item name="android:windowEnterAnimation">@anim/pop_enter</item>
        <item name="android:windowExitAnimation">@anim/pop_out</item>
    </style>

    <style name="leftButton">
        <item name="android:layout_width">50dp</item>
        <item name="android:layout_height">35dp</item>
        <item name="android:background">@drawable/topbutton_sel</item>
        <item name="android:textSize">15dp</item>
        <item name="android:textColor">@android:color/white</item>
        <item name="android:layout_marginLeft">10dp</item>
    </style>

    <style name="rightButton">
        <item name="android:layout_width">50dp</item>
        <item name="android:layout_height">35dp</item>
        <item name="android:background">@drawable/topbutton_sel</item>
        <item name="android:textSize">15dp</item>
        <item name="android:textColor">@android:color/white</item>
        <item name="android:layout_marginRight">10dp</item>
    </style>

    <style name="title">
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:textSize">22dp</item>
        <item name="android:textColor">@android:color/white</item>
    </style>

    <style name="cmtedit">
        <item name="android:layout_height">40dp</item>
        <item name="android:textSize">14sp</item>
        <item name="android:textColor">@android:color/black</item>
        <item name="android:singleLine">true</item>
        <item name="android:paddingLeft">10dp</item>
        <item name="android:paddingRight">10dp</item>
        <item name="android:background">@drawable/shuru</item>
    </style>

    <style name="radio_button">
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">35dp</item>
        <item name="android:drawablePadding">10dp</item>
        <item name="android:textSize">@dimen/text_common</item>
        <item name="android:paddingLeft">@dimen/body_margin_left_new</item>
    </style>

    <style name="check_button">
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">30dp</item>
        <item name="android:layout_marginLeft">@dimen/check_button_margin</item>
        <item name="android:layout_marginRight">@dimen/check_button_margin</item>
        <item name="android:layout_marginTop">@dimen/check_button_margin</item>
        <item name="android:background">@drawable/view_border_green_04</item>
        <item name="android:text">@string/agreement_reigter_back</item>
        <item name="android:textColor">@color/text_green</item>
        <item name="android:textSize">@dimen/text_common</item>
    </style>

    <style name="qclass_tab">
        <item name="android:layout_width">0dp</item>
        <item name="android:layout_height">match_parent</item>
        <item name="android:textSize">@dimen/text_common</item>
        <item name="android:gravity">center</item>
    </style>

    <style name="qclass_cmd_send">
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:lineSpacingExtra">5dp</item>
    </style>

    <style name="share_ll">
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:gravity">center_vertical</item>
        <item name="android:orientation">horizontal</item>
        <item name="android:padding">@dimen/margin_top</item>
    </style>

    <style name="share_ll_item">
        <item name="android:layout_width">0dp</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:orientation">vertical</item>
        <item name="android:gravity">center</item>
    </style>

    <style name="pop_anim_style_up_and_down">
        <item name="android:windowEnterAnimation">@anim/pop_ver_up</item>
        <item name="android:windowExitAnimation">@anim/pop_ver_down</item>
    </style>

    <style name="pop_anim_style_down_and_up">
        <item name="android:windowEnterAnimation">@anim/pop_enter</item>
        <item name="android:windowExitAnimation">@anim/pop_out</item>
    </style>

    <style name="pop_anim_style_left_and_right">
        <item name="android:windowEnterAnimation">@anim/pop_hor_left</item>
        <item name="android:windowExitAnimation">@anim/pop_hor_right</item>
    </style>

    <style name="share_textview" parent="qclass_cmd_send">
        <item name="android:layout_marginTop">@dimen/margin_top</item>
        <item name="android:textSize">@dimen/text_common</item>
    </style>

    <style name="text_home" parent="qclass_cmd_send">
        <item name="android:layout_marginLeft">25dp</item>
        <item name="android:textColor">@color/text_white</item>
        <item name="android:textSize">@dimen/text_big</item>
        <item name="android:layout_centerVertical">true</item>
    </style>

    <style name="img_home" parent="text_home">
        <item name="android:layout_marginRight">17dp</item>
        <item name="android:lineSpacingExtra">5dp</item>
        <item name="android:layout_alignParentRight">true</item>
    </style>

    <style name="qclass_status_pop" parent="qclass_cmd_send">
        <item name="android:paddingTop">15dp</item>
        <item name="android:paddingBottom">15dp</item>
        <item name="android:paddingLeft">35dp</item>
        <item name="android:paddingRight">35dp</item>
        <item name="android:background">@drawable/menu_item_bg</item>
        <item name="android:textColor">@color/text_black</item>
        <item name="android:textSize">@dimen/text_very_big</item>
    </style>

    <style name="text_index" parent="qclass_cmd_send">
        <item name="android:layout_centerHorizontal">true</item>
        <item name="android:layout_marginTop">@dimen/margin_top_big</item>
        <item name="android:textColor">@color/text_black</item>
        <item name="android:textSize">@dimen/text_common</item>
    </style>
    <style name="text_base" parent="qclass_cmd_send">
        <item name="android:textSize">@dimen/text_common</item>
        <item name="android:textColor">@color/text_gray</item>
        <item name="android:layout_marginTop">@dimen/margin_top</item>
    </style>
    <style name="food_relate" parent="qclass_cmd_send">
        <item name="android:layout_marginLeft">1dp</item>
        <item name="android:layout_marginTop">1dp</item>
        <item name="android:scaleType">centerCrop</item>
        <item name="android:layout_width">79dp</item>
        <item name="android:layout_height">79dp</item>
    </style>
</resources>